		if ("activate2" == 0) { 
			set	"activate2" "1" ; 
			if ("gui::inGame" == 0) {
				resetTime "MenuMusic" "0" ;
			}			
			if ("gui::inGame" >= 1) {
				resetTime "MenuMusic" "0" ;
			}
		}
	}

